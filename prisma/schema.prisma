generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DiscordUser {
  id               Int                @unique
  RiotVerification RiotVerification[]
}

model RiotVerification {
  id Int @id @default(autoincrement())

  discordUser   DiscordUser @relation(fields: [discordUserId], references: [id])
  discordUserId Int
}
